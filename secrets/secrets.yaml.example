# Example secrets file
# This is a template showing the structure for your actual secrets.yaml
#
# To create your encrypted secrets.yaml:
# 1. Copy this file: cp secrets.yaml.example secrets.yaml
# 2. Edit with sops: sops secrets/secrets.yaml
# 3. Replace placeholder values with your actual secrets
# 4. Save and exit - sops will encrypt it automatically

# API Keys for various services
api_keys:
  # Tavily API Key (for web search)
  tavily: "your-tavily-api-key-here"

  # Morph API Key
  morph: "your-morph-api-key-here"

# You can add more secrets as needed:
# tokens:
#   github: "your-github-token"
#   gitlab: "your-gitlab-token"
#
# database:
#   password: "your-db-password"
#   connection_string: "postgresql://..."
