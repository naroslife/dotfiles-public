+# Parse command line arguments
+parse_arguments() {
+	while  [[ $# -gt 0 ]]; do
+		case $1 in
+			-y |      --yes)
+				ASSUME_YES=true
+				shift
+				;;
+			-b |      --backup)
+				CREATE_BACKUPS=true
+				shift
+				;;
+			--no-backup)
+				CREATE_BACKUPS=false
+				shift
+				;;
+			-k |      --keep-nix)
+				KEEP_NIX=true
+				shift
+				;;
+			-v |      --verbose)
+				VERBOSE=true
+				shift
+				;;
+			-n |      --dry-run)
+				DRY_RUN=true
+				shift
+				;;
+			-h |      --help)
+				show_help
+				exit           0
+				;;
+			*)
+				die           "Unknown option: $1. Use --help for usage information."
+				;;
+		esac
+	done
+}
