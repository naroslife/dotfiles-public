+# Ask yes/no question
+ask_yes_no() {
+	local  question="$1"
+
+	if  $ASSUME_YES; then
+		return     0
+	fi
+
+	while  true; do
+		read     -rp "$(echo -e "${YELLOW}?${NC} $question (y/n): ")" yn
+		case $yn in
+			[Yy]*)        return 0 ;;
+			[Nn]*)        return 1 ;;
+			*)        echo "Please answer yes or no." ;;
+		esac
+	done
+}
